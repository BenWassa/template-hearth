{
  "name": "hearth",
  "version": "3.7.1",
  "private": true,
  "homepage": "https://benwassa.github.io/hearthMVP",
  "dependencies": {
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.4.0",
    "three": "^0.167.1"
  },
  "scripts": {
    "dev": "npm start",
    "start": "BROWSER=none react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:ci": "eslint \"src/**/*.{js,jsx}\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json,md,html}\" \"public/**/*.{js,css,json,md,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,json,md,html}\" \"public/**/*.{js,css,json,md,html}\"",
    "check:lockfile": "npm ci --dry-run",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "nth-check": "^2.0.1",
    "postcss": "^8.4.31",
    "svgo": "^2.8.0",
    "css-select": "^4.1.0",
    "undici": "^6.21.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "eslint": "^8.46.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^2.8.8",
    "react-scripts": "^5.0.1",
    "sharp": "^0.32.3",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.19",
    "web-streams-polyfill": "^4.2.0",
    "yaml": "^2.8.2"
  }
}
